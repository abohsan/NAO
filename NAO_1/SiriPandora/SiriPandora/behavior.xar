                            <script language="4">
                                <content>
                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.manager=ALProxy("ALBehaviorManager")
        self.storage=ALProxy("ALMemory")
        self.tts = ALProxy("ALTextToSpeech")
        self.fm = ALProxy("ALFrameManager")

        self.StopFlag = False
    def onLoad(self):
        pass

    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self):
        filename  = self.getParameter("Input File")
        if filename=="":
            self.log("Filename error:" + filename)
            self.onError()
        else:
            self.log("Filename:" + filename)
            try:
                fullpath = self.fm.getBehaviorPath(self.behaviorId) + filename
                self.log("Input file:" + fullpath)
                input = open(fullpath, "r")
                lstchar = ' '
                stemp = "XXXXX"
                blist = []
                while lstchar <> '':
                    while stemp[0:3] <> ">B<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    bhtemp = ""
                    while lstchar <> '<' and lstchar <> "\n" and  lstchar <> '':
                        bhtemp = bhtemp + lstchar
                        lstchar = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:3]) <> ">K<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    kytemp = ""
                    while lstchar <> '<' and lstchar <> "\n" and  lstchar <> '':
                        kytemp = kytemp + lstchar
                        lstchar = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:3]) <> ">C<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    cttemp = ""
                    while lstchar <> '<' and lstchar <> "\n" and lstchar <> "\r" and  lstchar <> '':
                        cttemp = cttemp + lstchar
                        lstchar = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:5]) <> ">WRW<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    if str(stemp[0:5]) == ">WRW<":
                        wrwtemp = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:4]) <> ">MM<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    if str(stemp[0:4]) == ">MM<":
                        MMtemp = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:4]) <> ">RT<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    trtemp = ""
                    while lstchar <> '<' and lstchar <> "\n" and lstchar <> "\r" and  lstchar <> '':
                        trtemp = trtemp + lstchar
                        lstchar = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:4]) <> ">RR<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    rrtemp = ""
                    while lstchar <> '<' and lstchar <> "\n" and lstchar <> "\r" and  lstchar <> '':
                        rrtemp = rrtemp + lstchar
                        lstchar = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:5]) <> ">IAM<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    if str(stemp[0:5]) == ">IAM<":
                        maitemp = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:5]) <> ">TIR<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    if str(stemp[0:5]) == ">TIR<":
                        rittemp = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:5]) <> ">SAR<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    rastemp = ""
                    while lstchar <> '<' and lstchar <> "\n" and lstchar <> "\r" and  lstchar <> '':
                        rastemp = rastemp + lstchar
                        lstchar = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:4]) <> ">TS<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    sttemp =  ""
                    while lstchar <> '<' and lstchar <> "\n" and lstchar <> "\r" and  lstchar <> '':
                        sttemp = sttemp + lstchar
                        lstchar = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:4]) <> ">RI<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    if str(stemp[0:4]) == ">RI<":
                        irtemp = input.read(1)
                    stemp = lstchar + stemp[0:5]
                    while str(stemp[0:3]) <> ">F<" and lstchar <> '':
                        lstchar = input.read(1)
                        stemp = lstchar + stemp
                    lstchar = input.read(1)
                    ftemp = ""
                    while lstchar <> '<' and lstchar <> "\n" and lstchar <> "\r" and  lstchar <> '':
                        ftemp = ftemp + lstchar
                        lstchar = input.read(1)
                    if bhtemp <> "":
                        tempbl = []
                        tempbl.append(bhtemp)
                        tempbl.append(kytemp)
                        tempbl.append(cttemp)
                        tempbl.append(wrwtemp)
                        tempbl.append(MMtemp)
                        tempbl.append(trtemp)
                        tempbl.append(rrtemp)
                        tempbl.append(maitemp)
                        tempbl.append(rittemp)
                        tempbl.append(rastemp)
                        tempbl.append(sttemp)
                        tempbl.append(irtemp)
                        tempbl.append(ftemp)
                        blist.append(";".join(tempbl))
                output = "@".join(blist)
                self.storage.insertData("AIBehaviorList", output)
                input.close()
                self.onStopped() #~ activate output of the box
            except:
                self.log("Error opening/reading file")
                self.onError()
    def onInput_onStop(self):
        self.StopFlag=True
        self.onUnload() #~ it is recommanded to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass]]>
