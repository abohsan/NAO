                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import re
import urllib
import urllib2
import xml.dom.minidom

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.myLogger = ALProxy("ALLogger")
        self.custid = ALMemory.getData("Device/DeviceList/ChestBoard/BodyNickName")

    def onLoad(self):
        pass

    def onUnload(self):
        pass

    def pandorabots_request(self, text):
        url = "http://www.pandorabots.com/pandora/talk-xml"
        values = {'botid' : self.getParameter("botid"),
                       'custid' : self.custid,
                       'input' : text,
                      }
        try:
            data = urllib.urlencode(values)
            req = urllib2.Request(url, data)
            response = urllib2.urlopen(req)
            content = xml.dom.minidom.parseString(response.read())
            return str(content.getElementsByTagName('that')[0].firstChild.data.encode("utf-8"))
        except Exception, detail:
            return detail

    def cleanup_text(self, data):
        data = re.sub(r'<[^<].*?/?>', '', data)    # remove html tags
        data = re.sub(r'\s+', ' ', data)    # remove extra spaces
        data = data.replace(", seeker", "")    # remove word 'seeker'
        data = data.replace(" .", ".")    # remove "space dot"
        return data

    def onInput_onStart(self, p):
        result = self.pandorabots_request(str(p))
        result = self.cleanup_text(result)
        self.myLogger.info("PandoraBots Connect", result)
        self.onStopped(result)

    def onInput_onStop(self):
        self.onUnload()
        pass]]>
</content>
