                                                        <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.storage=ALProxy("ALMemory")
        self.tts=ALProxy("ALTextToSpeech")
        self.aKeys = []

    def onLoad(self):
        #~ puts code for box initialization here
        pass

    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self, p):
        sentence = str(p).upper()
        wlist = sentence.split(" ")
        behaviorkeys = self.getParameter("RunBehaviorKeyWords")
        behaviorkeys = behaviorkeys.upper()
        bkeylist = behaviorkeys.split(";")
        bIsBehavior = False
        blKey = False
        blIndex = 0
        for word in wlist:
            for keyword in bkeylist:
                if keyword == word:
                    bIsBehavior = True
            tempblIndex = 0
            for keybehaviorword in self.aKeys:
                if keybehaviorword.upper() == word:
                    blKey = True
                    blIndex = tempblIndex
                tempblIndex = tempblIndex + 1
        if bIsBehavior and blKey:
            self.BehaviorCalled(blIndex)
        else:
            self.NotBehavior(str(p))
        self.onStopped() #~ activate output of the box
        pass

    def onInput_onStop(self):
        self.onUnload() #~ it is recommanded to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass

    def onInput_KeyWordList(self, p):
        self.aKeys = p
        pass]]>
</content>
