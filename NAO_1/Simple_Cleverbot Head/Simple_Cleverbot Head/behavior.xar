                                                <script language="4">
                                                    <content>
                                                        <![CDATA[import subprocess, os, re, urllib2

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #~ puts code for box initialization here
        try:
            import poster
        except:
            import sys
            sys.path.append(ALFrameManager.getBehaviorPath(self.behaviorId))
            import poster
        self.poster = poster
    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self):
        os.chdir("/home/nao/recordedSounds")
        try:
            subprocess.check_call(["ffmpeg", "-y", "-i", "recording.ogg", "-acodec", "pcm_s16le", "out.wav"])
            subprocess.check_call(["flac", "out.wav", "-f", "--best", "--sample-rate=16000", "-o", "out.flac"])

            # Register the streaming http handlers with urllib2
            self.poster.streaminghttp.register_openers()
            # Encode audio file
            datagen, headers = self.poster.encode.multipart_encode({"audio": open("/home/nao/recordedSounds/out.flac", "rb")})
            headers['Content-Type']= 'audio/x-flac; rate=16000'
            # Create the Request object
            request = urllib2.Request("http://www.google.com/speech-api/v1/recognize?lang=en", datagen, headers)
            # Actually do the request, and get the response
            response = urllib2.urlopen(request, timeout=10).read()
            # Parese response
            response = [e.split(':') for e in re.sub(r'[]{[}\n"]|(utterance":)', '', response).split(',')]
            # Output response
            self.Transcript(response[2][1])
            self.Confidence(float(response[3][1]))

        except:
            ##self.Transcript("I did not understand.")
            self.Confidence(0)
            self.Error()

    def onInput_onStop(self):
        self.onUnload() #~ it is recommanded to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass]]>
</content>
