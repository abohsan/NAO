                            <Output name="Transcript" type="3" type_size="1" nature="2" inner="0" tooltip="String transcribing the input speech" id="3" />
                            <Output name="Confidence" type="2" type_size="1" nature="2" inner="0" tooltip="Value between 0 and 1 corresponding to the level of confidence of the transcript" id="4" />
                            <Output name="Error" type="1" type_size="1" nature="2" inner="0" tooltip="Something went wrong with the recognition. Either the word was not recognized or there is no internet connection." id="5" />
